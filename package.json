{
	"name": "scorecard",
	"version": "0.0.0",
	"description": "A scorecard module",
	"main": "lib/index.js",
	"style": "lib/main.css",
	"repository": "git://github.com/bdo-labs/scorecard.git",
	"license": "MIT",
	"dependencies": {
		"services": "git+https://edbdo.visualstudio.com/DefaultCollection/BDO%20Labs/_git/Services",
		"filters": "git+ssh://git@github.com:bdo-labs/filters.git"
	},
	"scripts": {
		"test": "karma start"
	},
	"atomify": {
